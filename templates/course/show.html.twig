{% extends 'base.html.twig' %}

{% block title %}Course{% endblock %}

{% block body %}
    <h1>Course</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ course.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ course.date }}</td>
            </tr>
            <tr>
                <th>H_debut</th>
                <td>{{ course.hDebut }}</td>
            </tr>
            <tr>
                <th>H_fin</th>
                <td>{{ course.hFin }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ course.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_course_index') }}">back to list</a>

    <a href="{{ path('app_course_edit', {'id': course.id}) }}">edit</a>

    {{ include('course/_delete_form.html.twig') }}
{% endblock %}
